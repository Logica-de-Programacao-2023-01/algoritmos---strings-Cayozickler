package main

import "fmt"

func main() {
	var str1, str2 string

	// Solicita a primeira string ao usuário
	fmt.Print("Digite a primeira string: ")
	fmt.Scanln(&str1)

	// Solicita a segunda string ao usuário
	fmt.Print("Digite a segunda string: ")
	fmt.Scanln(&str2)

	// Verifica se as strings são iguais
	if str1 == str2 {
		fmt.Println("As strings são iguais.")
	} else {
		fmt.Println("As strings são diferentes.")
	}
}
